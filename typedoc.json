{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "excludePrivate": true,
  "out": ".docs/API",
  "sort": ["alphabetical"],
  "plugin": ["typedoc-plugin-missing-exports"],
  "tsconfig": "tsconfig.build.json",
  "useTsLinkResolution": false,
  "excludeInternal": true,
  "excludeExternals": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  }
}
